  color: #beda42;
}
.haveimgview{
  width: 100%;
  height: 100%;
}
.viewTop{
  width: 100%;
  height: 380rpx;
  display: flex;
  flex-direction: column; 
  justify-content: center;
  align-items: center;
}
.renwutouxiang{
  width: 220rpx;
  height: 220rpx;
