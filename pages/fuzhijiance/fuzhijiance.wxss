  width: 32rpx;
  height: 39rpx;
  margin-left:40rpx;
}
.baogaotxt{
  color: #ffffff;
  /* font-weight: 900; */
  margin-left: 16rpx;
}
.fuzhicontentview{
  width: 100%;
  height: 100%;
  padding: 0 40rpx;
  box-sizing: border-box;
}
.fuse,.fuzhi,.heitoushuliang,.maokongzhaungkuang,.doudoushuliang,.naishouxing{
  width: 100%;
  min-height: 300rpx;
  padding: 80rpx 0;
  border-bottom: 1rpx solid #cccccc;
}
.naishouxing{
  border: none;
}
.fusebigtitle,.fuzhibigtitle,.heitoubigtitle,.maokongbigtitle,.doudoubigtitle,.naishoubigtitle{
  font-size: 42rpx;
  color: #000000;
  font-weight: 900;
  margin-bottom: 30rpx;
}
.fusedengji,.fuzhidengji,.heitoudengji,.maokongdengji,.doudoudengji,.naishoudengji{
  padding-left: 60rpx;
  box-sizing: border-box;
  color: #beda42;
  font-size: 40rpx;
  margin-bottom: 20rpx;
}
.fusecontent,.fuzhicontent,.heitoucontent,.maokongconthent,.doudoucontent,.naishoucontent{
  padding: 0 60rpx;
  box-sizing: border-box;
  color: #bbbbbb;
  line-height: 56rpx;
}
